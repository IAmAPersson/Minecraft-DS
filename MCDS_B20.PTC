@DRAWGROUND
C=COS(CA)
S=SIN(CA)
FOR J=96TO 191STEP FLOOR(Q/2)+1
K=J+FLOOR(Q/2)
Z=50/(J-94)
I=Z/55
M=I*S
N=-I*C
X=CX+Z*C-M*32
Y=CY+Z*S-N*32
FOR F=ABS(SWV-128)TO SWV+132STEP 4
X=X+M
Y=Y+N
IF!(X OR Y AND-64)THEN GFILL F,J,F+3,K,T(M(X,Y)+Y%1*16,X%1*16)
NEXT
NEXT
RETURN

