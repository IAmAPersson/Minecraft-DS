@SLIME
FOR I=0TO MOB_COUNT-1
MOB_NX=MOB_X(I)
MOB_NY=MOB_Y(I)
MOB_LAYER(I)=!MOB_LAYER(I)
IF RND(4)==3THEN MOB_DIRECTION(I)=RND(4)
IF MOB_DIRECTION(I)==0THEN MOB_NX=MOB_X(I)-1
IF MOB_DIRECTION(I)==1THEN MOB_NX=MOB_X(I)+1
IF MOB_DIRECTION(I)==2THEN MOB_NY=MOB_Y(I)-1
IF MOB_DIRECTION(I)==3THEN MOB_NY=MOB_Y(I)+1
IF FLOOR(CAMX+.5)==MOB_NX AND FLOOR(CAMY+.5)==MOB_NY THEN MOB_LAYER(I)=!MOB_LAYER(I):GOTO@SSKIPL
IF MAP(MOB_NX,MOB_NY)OR MAPB(MOB_NX,MOB_NY)THEN MOB_LAYER(I)=!MOB_LAYER(I):GOTO@SSKIPL
IF MOB_LAYER(I)THEN MAP(MOB_X(I),MOB_Y(I))=0
IF !MOB_LAYER(I) THEN MAPB(MOB_X(I),MOB_Y(I))=0
MOB_X(I)=MOB_NX
MOB_Y(I)=MOB_NY
IF !MOB_LAYER(I)AND !INNETHER THEN MAP(MOB_X(I),MOB_Y(I))=11
IF MOB_LAYER(I)AND !INNETHER THEN MAPB(MOB_X(I),MOB_Y(I))=11
IF !MOB_LAYER(I)AND INNETHER THEN MAP(MOB_X(I),MOB_Y(I))=34
IF MOB_LAYER(I)AND INNETHER THEN MAPB(MOB_X(I),MOB_Y(I))=34
@SSKIPL
NEXT
RETURN

